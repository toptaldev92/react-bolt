    "name": "react-bolt",
    "version": "1.0.0",
    "description": "âš¡ The most simple & robust boilerplate for your React projects.",
    "main": "src/index.js",
    "scripts": {
        "start": "webpack-dev-server --hot --mode development ./src/index.js --output ./dist/main.js --watch",
        "build": "babel src/ -d lib/",
        "prepublish": "npm run build",
        "test": "jest --coverage --config=jest.config.json",
        "test:watch": "jest --watch --coverage",
        "flow": "flow"
    },
    "lint-staged": {
        "*.{js,css}": [
            "prettier --write",
            "git add"
        ],
        "*.js": [
            "eslint src --fix",
            "git add"
        ]
